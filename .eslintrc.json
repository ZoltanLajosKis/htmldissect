{
  "root": true,
  "extends": [
    "eslint:recommended"
  ],
  "plugins": [
    "node"
  ],
  "env": {
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6
  },
  "rules": {
    "accessor-pairs": 2,
    "array-bracket-spacing": [2, "never"],
    "array-callback-return": 2,
    "block-scoped-var": 2,
    "block-spacing": "error",
    "brace-style": [1, "1tbs"],
    "camelcase": 2,
    "comma-dangle": [2, "never"],
    "comma-spacing": ["error", { "before": false, "after": true }],
    "computed-property-spacing": [2, "never"],
    "consistent-return": 1,
    "curly": 2,
    "dot-notation": [2, { "allowKeywords": false }],
    "eol-last": 2,
    "eqeqeq": [2, "smart"],
    "global-require": 2,
    "handle-callback-err": 1,
    "indent": 2,
    "key-spacing": 2,
    "keyword-spacing": [2, {"before": true, "after": true}],
    "linebreak-style": [2, "unix"],
    "max-nested-callbacks": [2, 5],
    "max-statements-per-line": [2, { "max": 3 }],
    "new-cap": 2,
    "new-parens": 2,
    "no-alert": 2,
    "no-array-constructor": 2,
    "no-caller": 2,
    "no-catch-shadow": 2,
    "no-confusing-arrow": 2,
    "no-console": 2,
    "no-const-assign": 2,
    "no-debugger": 2,
    "no-delete-var": 2,
    "no-div-regex": 1,
    "no-dupe-args": 2,
    "no-dupe-class-members": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-duplicate-imports": 2,
    "no-else-return": 1,
    "no-empty-character-class": 2,
    "no-empty": [2, { "allowEmptyCatch": true }],
    "no-empty-function": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-ex-assign": 2,
    "no-extend-native": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-label": 2,
    "no-extra-parens": 2,
    "no-extra-semi": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-func-assign": 2,
    "no-implied-eval": 2,
    "no-inner-declarations": 2,
    "no-irregular-whitespace": 2,
    "no-label-var": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-lonely-if": 2,
    "no-mixed-requires": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multiple-empty-lines": [2, {"max": 3}],
    "no-multi-spaces": 1,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-negated-in-lhs": 2,
    "no-nested-ternary": 2,
    "no-new": 2,
    "no-new-object": 2,
    "no-new-require": 2,
    "no-new-wrappers": 2,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-octal-escape": 2,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-return-assign": 2,
    "no-self-assign": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-shadow": 2,
    "no-spaced-func": 2,
    "no-throw-literal": 2,
    "no-trailing-spaces": 2,
    "no-undef": 2,
    "no-undefined": 2,
    "no-undef-init": 2,
    "no-unmodified-loop-condition": 1,
    "no-unneeded-ternary": 2,
    "no-unreachable": 2,
    "no-unused-expressions": 2,
    "no-unused-labels": 2,
    "no-unused-vars": 2,
    "no-use-before-define": [2, "nofunc"],
    "no-useless-call": 2,
    "no-useless-concat": 2,
    "no-useless-constructor": 2,
    "no-useless-escape": 2,
    "no-var": 2,
    "no-void": 2,
    "no-whitespace-before-property": 2,
    "no-with": 2,
    "object-curly-spacing": [2, "never"],
    "object-property-newline": [2, { "allowMultiplePropertiesPerLine": true }],
    "object-shorthand": 2,
    "prefer-rest-params": 2,
    "quote-props": [2, "as-needed"],
    "radix": 2,
    "semi": [2, "always"],
    "semi-spacing": 2,
    "space-before-function-paren": [2, {"anonymous": "never", "named": "never"}],
    "space-infix-ops": 2,
    "space-in-parens": [2, "never"],
    "space-unary-ops": 2,
    "use-isnan": 2,
    "valid-typeof": 2,
    "wrap-iife": 2
  }
}
